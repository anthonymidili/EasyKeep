<%= form_for(@company) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>

  <div class="row">
    <div class="small-12 large-6 columns">
      <%= f.label :company_name %>
      <%= f.text_field :company_name, autofocus: true %>
    </div>
  </div>

  <%= render 'shared/address_fields', f: f %>
<% end %>

<%= form_for [@account, @service] do |f| %>
  <%= render 'shared/error_messages', object: f.object %>

  <div class="field">
    <%= f.label :performed_on %><br />
    <%= f.date_select :performed_on %>
  </div>
  <div class="field">
    <%= f.label :money_received %><br />
    <%= f.text_field :money_received %>
  </div>
  <div class="field">
    <%= f.label :note %><br />
    <%= f.text_area :note %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>

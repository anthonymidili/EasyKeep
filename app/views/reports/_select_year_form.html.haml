.row.hide_on_print
  = form_tag accounts_income_reports_path, method: :get do
    .small-12.medium-3.large-3.columns
      = label_tag 'date_year', 'Year'
      = select_date(active_date, discard_month: true, discard_day: true)

    .small-12.medium-3.large-3.medium-pull-6.large-pull-6.columns
      = submit_tag 'Select Year to View', class: 'tiny button radius expand tiny_button_down', name: nil

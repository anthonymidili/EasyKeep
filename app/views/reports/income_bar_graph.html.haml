- provide(:title, 'Income Report Graph')

.row
  .small-12.columns
    %br/
    .hide_on_print.outline
      %h4 Income - Bar Graph
      = render 'select_month_or_year_form'
      = render "select_#{quarter_or_year}_form"

    = content_tag :div, '', id: 'moneyOverTimeBar', style: 'width:100%; height:500px;',
      data: { amounts: current_company.total_payments_over_time(quarter_or_year, date_to_view) }

    = render 'inline_totals'
